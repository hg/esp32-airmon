[server]
http_addr = localhost
domain = {{ domain }}

[security]
admin_user = {{ grafana.user }}
admin_password = {{ grafana.password }}
secret_key = {{ grafana.secret_key }}
disable_gravatar = true
cookie_secure = true

[auth]
login_maximum_inactive_lifetime_duration = 10y
login_maximum_lifetime_duration = 10y

[users]
allow_sign_up = false
