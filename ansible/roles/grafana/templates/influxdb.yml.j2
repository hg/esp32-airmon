apiVersion: 1

datasources:
  - name: InfluxDB
    type: influxdb
    access: proxy
    url: http://localhost:8086
    isDefault: true
    secureJsonData:
      token: {{ tokens.influx }}
    jsonData:
      version: Flux
      organization: home
      defaultBucket: airmon
      tlsSkipVerify: true
    editable: true
